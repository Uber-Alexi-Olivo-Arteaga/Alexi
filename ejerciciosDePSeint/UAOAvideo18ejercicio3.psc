//Hacer un programa para intercambiar el valor de 2 variables

Proceso principal
	Definir a,b,aux Como Enteros; //Variable auxiliar
	
	Escribir "Digite el valor de a: ";
	Leer a;
	Escribir "Digite el valor de b: ";
	Leer b;
	
	aux <- a;
	a <- b;
	b <- aux;
	
	Escribir "Su nuevo valor de a es: ", a;
	Escribir "Su nuevo valor de b es: ", b;
FinProceso

